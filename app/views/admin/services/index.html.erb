<h1>Listing services</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Position</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @services.each do |service| %>
  <tr>
    <td><%= service.name %></td>
    <td><%= service.position %></td>
    <td><%= link_to 'Show', [:admin, service] %></td>
    <td><%= link_to 'Edit', edit_admin_service_path(service) %></td>
    <td><%= link_to 'Destroy', [:admin, service], method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Service', new_admin_service_path %>
